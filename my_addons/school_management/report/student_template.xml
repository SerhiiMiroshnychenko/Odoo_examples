<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_student">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1>Student Report</h1>
                        <p>Student:</p>
                        <p t-field="o.name"/>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
