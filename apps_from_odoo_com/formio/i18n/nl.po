#. module: formio
#: model:mail.template,body_html:formio.mail_invitation_internal_user
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Hello <t t-out=\"object.user_id.partner_id.name\"></t>,<br><br>\n"
"        You have been invited to fill-in the form: <t t-out=\"object.title"
"\"></t><br>\n"
"        Your response would be appreciated.<br><br>\n"
"        Click the button to go to the form (record), which requires you're "
"logged in.\n"
"        <div style=\"margin: 16px 0px 16px 0px;\">\n"
"            <a t-attf-href=\"{{ object.act_window_url }}\" style="
"\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: "
"none; color: #fff; border-radius: 5px; font-size:13px;\">\n"
"                Form\n"
"            </a>\n"
"        </div>\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Beste <t t-out=\"object.user_id.partner_id.name\">,<br/><br/>\n"
"        U bent uitgenodigd voor het invullen van het formulier: <t t-out="
"\"object.title/><br/>\n"
